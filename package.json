{
  "name": "alpha-date-automation",
  "private": true,
  "version": "1.0.0",
  "description": "Full-stack Alpha Date automation with Vite frontend and Express backend",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "npx vite build",
    "build:prod": "bash build.sh",
    "build:simple": "bash build-simple.sh",
    "build:fast": "bash build-fast.sh",
    "preview": "vite preview",
    "start": "node server.js",
    "server:dev": "nodemon server.js",
    "test:captcha": "node test-captcha.js",
    "test:captcha:manual": "node test-captcha.js --manual",
    "test:chrome": "node test-chrome.js",
    "test:api": "node test-api-only.js",
    "debug:chrome": "node -e \"const puppeteer = require('puppeteer'); console.log('Puppeteer version:', require('puppeteer/package.json').version); console.log('Chrome executable path:', puppeteer.executablePath());\""
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.6.11",
    "nodemon": "^2.0.22",
    "puppeteer": "^22.0.0",
    "puppeteer-core": "^22.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "serve": "^14.2.1",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
